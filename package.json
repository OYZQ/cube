{
  "name": "cube.css",
  "version": "1.3.2",
  "description": "Cube CSS framework",
  "private": true,
  "devDependencies": {
    "autoprefixer": "~6.1.2",
    "del": "~2.2.0",
    "glob": "~3.2.7",
    "gulp": "~3.9.1",
    "gulp-concat": "~2.5.2",
    "gulp-cssnano": "~2.0.0",
    "gulp-postcss": "~6.0.1",
    "gulp-rename": "~1.2.2",
    "gulp-sass": "~2.0.1",
    "gulp-size": "~2.0.0",
    "gulp-sourcemaps": "~1.6.0",
    "postcss-filter-gradient": "^0.2.0",
    "q": "~0.9.7",
    "request": "~2.27.0",
    "run-sequence": "~1.1.5",
    "semver": "~2.1.0",
    "stylelint": "^7.1.0",
    "stylelint-config-standard": "^12.0.0",
    "unzip": "~0.1.9"
  },
  "scripts": {
    "test": "stylelint src/scss/*.scss"
